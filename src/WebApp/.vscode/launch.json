{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Launch (WebApp portable)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/bin/Debug/net9.0/WebApp.dll",
      "cwd": "${workspaceFolder}",
      "env": {
        "ASPNETCORE_URLS": "http://localhost:9000",
        "ASPNETCORE_ENVIRONMENT": "VSCode_Development"
      },
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)",
        "uriFormat": "%s/Login"
      }, 
      "justMyCode": true
    }
  ]
}